{
  "name": "new-world-kids",
  "version": "1.0.0",
  "private": true,
  "description": "New World Kids - Empowering Youth Through Education, Technology & Community",
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "dev:web": "cd apps/web && yarn dev",
    "dev:timeline": "cd apps/timeline-app && yarn dev",
    "dev:cms": "cd services/cms && yarn develop",
    "dev:blockchain": "cd services/blockchain && yarn dev",
    "dev:agents": "cd services/stellar-agents && yarn dev",
    "build:web": "cd apps/web && yarn build",
    "build:timeline": "cd apps/timeline-app && yarn build",
    "build:cms": "cd services/cms && yarn build",
    "build:blockchain": "cd services/blockchain && yarn build",
    "start:web": "cd apps/web && yarn start",
    "start:cms": "cd services/cms && yarn start",
    "seed:cms": "node services/cms/scripts/seed/seed-data.js"
  },
  "dependencies": {
    "firebase": "^10.8.0",
    "framer-motion": "^11.0.8",
    "next": "^15.0.0",
    "next-intl": "^3.11.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "postcss": "^8",
    "turbo": "^2.5.8",
    "typescript": "^5"
  },
  "engines": {
    "node": "22.x",
    "yarn": "1.22.x"
  },
  "resolutions": {
    "date-fns": "3.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/executiveusa/strapi-template-new-world-kids.git"
  },
  "author": "New World Kids Team",
  "license": "MIT"
}
